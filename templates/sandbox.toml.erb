[<%= @name -%>]
type = "<%= @sandbox_type -%>"

# sandbox settings
script_type = "<%= @script_type -%>"
filename = "<%= @filename -%>"
<% unless [nil, :undefined, :undef].include?(@preserve_data) -%>preserve_data = <%= @preserve_data %>
<% end -%>
<% if @memory_limit then -%>memory_limit = <%= @memory_limit %>
<% end -%>
<% if @instruction_limit then -%>instruction_limit = <%= @instruction_limit %>
<% end -%>
<% if @output_limit then -%>output_limit = <%= @output_limit %>
<% end -%>
<% if @module_directory then -%>module_directory = "<%= @module_directory -%>"
<% end -%>

<% if @config then -%>[<%= @name -%>.config]
<% @config.sort.each do |key, value| -%>
<% unless [nil, :undefined, :undef].include?(value) -%><%= key -%> = <%= value.is_a?(String) ? '"'+value+'"' : value %>
<% end -%>
<% end -%>
<% end -%>
