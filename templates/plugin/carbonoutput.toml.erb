# This file is controlled via puppet.
[<%= @full_name -%>]
type = "CarbonOutput"

<%= scope.function_template(['heka/plugin/_output.toml.erb']) %>

# specific settings
address = "<%= @address -%>"
<% if @protocol then -%>protocol = "<%= @protocol -%>"
<% end %>
<% unless [nil, :undefined, :undef].include?(@tcp_keep_alive) then -%>tcp_keep_alive = <%= @tcp_keep_alive %>
<% end %>

<%= scope.function_template(['heka/plugin/_buffering.toml.erb']) %>
