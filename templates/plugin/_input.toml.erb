# common input settings
<% if @decoder  then -%>decoder = "<%= @decoder -%>"
<% end -%>
<% unless [nil, :undefined, :undef].include?(@synchronous_decode) -%>synchronous_decode = <%= @synchronous_decode %>
<% end -%>
<% unless [nil, :undefined, :undef].include?(@send_decode_failures) -%>send_decode_failures = <%= @send_decode_failures %>
<% end -%>
<% unless [nil, :undefined, :undef].include?(@can_exit) -%>can_exit = <%= @can_exit %>
<% end -%>
<% if @splitter then -%>splitter = "<%= @splitter -%>"
<% end -%>
<% unless [nil, :undefined, :undef].include?(@log_decode_failures) -%>log_decode_failures = <%= @log_decode_failures %>
<% end -%>
<% unless [nil, :undefined, :undef].include?(@use_tls) then -%>use_tls = <%= @use_tls %>
<% end %>
