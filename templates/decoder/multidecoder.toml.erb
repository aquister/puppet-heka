# This file is controlled via puppet.
[<%= @full_name -%>]
type = "MultiDecoder"
subs = [<%= subs.map! {|sub| "'"+sub+"'"}.join(",") -%>]
<% unless [nil, :undefined, :undef].include?(@log_sub_errors) -%>log_sub_errors = "<%= @log_sub_errors -%>"
<% end -%>
<% if @cascade_strategy then -%>cascade_strategy = "<%= @cascade_strategy -%>"
<% end -%>
