[<%= @name -%>]
type = "HekaFramingSplitter"
<%= scope.function_template(['heka/splitters/_splitter.toml.erb']) -%>
skip_authentication = "<%= skip_authentication -%>"

<% if @signer then -%>
<% @signer.each do |key, value| -%>
    [<%= @plugin_name -%>.signer.<%= key -%>]
    hmac_key = "<%= value %>"
<% end -%>
<% end -%>
